   "source": [
    "## 7. Análise de Data Drift\n",
    "\n",
    "### Tarefa 5: Detecte mudanças na distribuição dos dados"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Comparar distribuições de features\n",
    "def analyze_feature_drift(X_reference, X_current, feature_names, top_n=5):\n",
    "    \"\"\"Analisar drift em features\"\"\"\n",
    "    from scipy import stats\n",
    "    \n",
    "    drift_scores = []\n",
    "    \n",
    "    for feature in feature_names:\n",
    "        # Teste de Kolmogorov-Smirnov\n",
    "        ks_stat, p_value = stats.ks_2samp(\n",
    "            X_reference[feature], \n",
    "            X_current[feature]\n",
    "        )\n",
    "        \n",
    "        drift_scores.append({\n",
    "            'feature': feature,\n",
    "            'ks_statistic': ks_stat,\n",
    "            'p_value': p_value,\n",
    "            'drift_detected': p_value < 0.05\n",
    "        })\n",
    "    \n",
    "    drift_df = pd.DataFrame(drift_scores).sort_values('ks_statistic', ascending=False)\n",
    "    return drift_df\n",
    "\n",
    "# Analisar drift\n",
    "drift_analysis = analyze_feature_drift(X_train, X_prod, X.columns)\n",
    "\n",
    "print(\"=== TOP 10 FEATURES COM MAIOR DRIFT ===\")\n",
    "print(drift_analysis.head(10))"
   ]
