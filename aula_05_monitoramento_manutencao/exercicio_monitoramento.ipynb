    "should_retrain, reasons = retrain_strategy.should_retrain(\n",
    "    latest_alert, avg_drift, n_samples\n",
    ")\n",
    "\n",
    "print(\"\\n=== ANÁLISE DE RETREINAMENTO ===\")\n",
    "print(f\"Deve retreinar: {'SIM' if should_retrain else 'NÃO'}\")\n",
    "print(f\"\\nRazões:\")\n",
    "for reason in reasons:\n",
    "    print(f\"  - {reason}\")\n",
    "\n",
    "if should_retrain:\n",
    "    print(\"\\n✅ Retreinamento recomendado!\")\n",
